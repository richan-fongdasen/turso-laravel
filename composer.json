{
    "name": "richan-fongdasen/laravel-turso",
    "description": "This is my package laravel-turso",
    "keywords": [
        "Richan Fongdasen",
        "laravel",
        "laravel-turso"
    ],
    "homepage": "https://github.com/richan-fongdasen/laravel-turso",
    "license": "MIT",
    "authors": [
        {
            "name": "Richan Fongdasen",
            "email": "richan.fongdasen@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^11.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1",
        "orchestra/testbench": "^9.0.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.3",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpstan/phpstan-strict-rules": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "RichanFongdasen\\LaravelTurso\\": "src/",
            "RichanFongdasen\\LaravelTurso\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RichanFongdasen\\LaravelTurso\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-laravel-turso --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve --host=0.0.0.0"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RichanFongdasen\\LaravelTurso\\LaravelTursoServiceProvider"
            ],
            "aliases": {
                "LaravelTurso": "RichanFongdasen\\LaravelTurso\\Facades\\LaravelTurso"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
